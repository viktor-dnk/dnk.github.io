---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>Дорога не кончается</title>
    <description>Заметки географа</description>
    <image>
      <url>https://viktor-dnk.ru/img/example-logos/rss.png</url>
      <title>Дорога не кончается</title>
      <link>{{ '' | absolute_url }}</link>
      <width>88</width>
      <height>88</height>
      <description>Заметки географа</description>
    </image>    
    <language>ru</language>
    <link>{{ '' | absolute_url }}</link>
    <atom:link href="{{ 'feed.xml' | absolute_url }}" rel="self" type="application/rss+xml" />
    {% assign excerpt_length = site.excerpt_length | default: 50 %}
    {% for post in site.posts limit:20 %}
      <item>
        <title>{{ post.title | strip_html | xml_escape }}</title>
        <description>
          {% if post.share-img %}&lt;p&gt;&lt;img src="{{ post.share-img | prepend: site.baseurl | prepend: site.url }}" alt="{{ post.title | strip_html | xml_escape }}" class="webfeedsFeaturedVisual"&gt;&lt;/p&gt;{% endif %}{{ post.share-description }}
        </description>
        <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <link>{{ post.url | absolute_url }}</link>
        <category>{{post.tags | sort | join: ", "}}</category> 
        <guid isPermaLink="true">{{ post.url | absolute_url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
