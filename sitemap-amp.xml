---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9" xmlns:xhtml="http://www.w3.org/1999/xhtml" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1">    
{% for toponymy in site.toponymy %}
<url>
    <loc>{{ toponymy.url | absolute_url }}</loc>
        <image:image>
            <image:loc>{{ toponymy.share-img | absolute_url }}</image:loc>
            <image:caption>{{ toponymy.img-caption | xml_escape }}</image:caption>
            <image:title>{{ toponymy.title | xml_escape }}</image:title>
            {% if toponymy.location %}<image:geo_location>{{ toponymy.location }}</image:geo_location>{% endif %}                  
        </image:image>
    <xhtml:link rel="amphtml" href="{{ toponymy.url | absolute_url | replace:'.io/','.io/amp/' }}" />
    <lastmod>{{ toponymy.last_modified_at | date_to_xmlschema }}</lastmod>
</url>
{% endfor %}

{% for mysteries-dolmens in site.mysteries-dolmens %}
<url>
    <loc>{{ mysteries-dolmens.url | absolute_url }}</loc>
        <image:image>
            <image:loc>{{ mysteries-dolmens.share-img | absolute_url }}</image:loc>
            {% if mysteries-dolmens.img-caption %}<image:caption>{{ mysteries-dolmens.img-caption }}</image:caption>{% endif %}
            <image:title>{{ mysteries-dolmens.title | xml_escape }}</image:title>
            {% if mysteries-dolmens.location %}<image:geo_location>{{ mysteries-dolmens.location }}</image:geo_location>{% endif %}            
        </image:image>
    <xhtml:link rel="amphtml" href="{{ mysteries-dolmens.url | absolute_url | replace:'.io/','.io/amp/' }}" />
    <lastmod>{{ mysteries-dolmens.last_modified_at | date_to_xmlschema }}</lastmod>
</url>
{% endfor %}

{% for category in site.categories %}
    {% capture category_link %} blog/{{ category[0] }}/index.html {% endcapture %}
    <url>
        <loc>{{ site.url }}/{{ category[0] }}/</loc>
    </url>
{% endfor %}
    <url>
        <loc>https://viktor-dnk.github.io/</loc>
    </url>
    <url>
        <loc>https://viktor-dnk.github.io/aboutme/</loc>
    </url>
    <url>
        <loc>https://viktor-dnk.github.io/thisblog/</loc>
    </url>
    <url>
        <loc>https://viktor-dnk.github.io/search/</loc>
    </url>
    <url>
        <loc>https://viktor-dnk.github.io/info/telegram/</loc>
    </url>     
</urlset>
