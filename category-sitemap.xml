---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:image="http://www.google.com/schemas/sitemap-image/1.1" xsi:schemaLocation="http://www.sitemaps.org/schemas/sitemap/0.9 http://www.sitemaps.org/schemas/sitemap/0.9/sitemap.xsd" xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}{% if page.url contains '/toponymy/' %}
	<url>
    <loc>{{ page.url | absolute_url }}</loc>
    {% if page.date %}<lastmod>{{ page.date | date_to_xmlschema }}</lastmod>{% endif %}
    <image:image>
    <image:loc>{{ page.share-img | absolute_url }}</image:loc>
    <image:caption>{{ page.img-caption | xml_escape }}</image:caption>
    <image:title>{{ page.title | xml_escape }}></image:title>
    {% if page.location %}<image:geo_location>{{ toponymy.location }}</image:geo_location>{% endif %}                  
    </image:image>
	</url>{% endif %}{% endfor %}
</urlset>
