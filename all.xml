---
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:dc="http://purl.org/dc/elements/1.1/">
	<channel>
		<title>{{ site.title | xml_escape }}</title>
		<description>{% if site.description %}{{ site.description | xml_escape }}{% endif %}</description>		
		<link>{{ site.url }}</link>
		<atom:link href="{{ 'feed.xml' | absolute_url }}" rel="self" type="application/rss+xml" />
		{% for toponymy in site.toponymy %}
			<item>
				<title>{{ toponymy.title | xml_escape }}</title>
				{% if toponymy.author.name %}
					<dc:creator>{{ toponymy.author.name | xml_escape }}</dc:creator>
				{% endif %}        
				<description>{{ toponymy.content | xml_escape }}</description>
				<pubDate>{{ toponymy.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
				<link>{{ site.url }}{{ toponymy.url }}</link>
				<guid isPermaLink="true">{{ site.url }}{{ toponymy.url }}</guid>
				{% for category in toponymy.categories | split: " " %}
					<category>{{ category }}</category>
				{% endfor %}
			</item>
		{% endfor %}
		
		{% for mysteries-dolmens in site.mysteries-dolmens %}
			<item>
				<title>{{ mysteries-dolmens.title | xml_escape }}</title>
				{% if mysteries-dolmens.author.name %}
					<dc:creator>{{ mysteries-dolmens.author.name | xml_escape }}</dc:creator>
				{% endif %}        
				<description>{{ mysteries-dolmens.content | xml_escape }}</description>
				<pubDate>{{ mysteries-dolmens.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
				<link>{{ site.url }}{{ mysteries-dolmens.url }}</link>
				<guid isPermaLink="true">{{ site.url }}{{ mysteries-dolmens.url }}</guid>
				{% for category in mysteries-dolmens.categories | split: " " %}
					<category>{{ category }}</category>
				{% endfor %}
			</item>
		{% endfor %}
		
	</channel>
</rss>
