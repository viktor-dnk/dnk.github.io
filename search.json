---
layout: nil
includeInSearch: false
---
{% assign pages = site.pages | where:"includeInSearch", "true" %}
[
	{% for mysteries-dolmens in site.mysteries-dolmens %},
	{
	"type": "page",
	"title": "{{ mysteries-dolmens.title }}",
	"categories": [{% for cat in mysteries-dolmens.categories %}"{{ cat }}"{% unless forloop.last %},{% endunless %}{% endfor %}],	
	"tags": [{% for tag in mysteries-dolmens.tags %}"{{ tag }}"{% unless forloop.last %},{% endunless %}{% endfor %}],
	"url": "{{ mysteries-dolmens.url }}",
	"content":{{ mysteries-dolmens.content | strip_html | json_escape | jsonify}},
	"excerpt": {{ mysteries-dolmens.excerpt | rstrip | json_escape | jsonify }},
	"description": {{ mysteries-dolmens.description | json_escape | jsonify }}
    }
    {% endfor %}
	
	{% for toponymy in site.toponymy %},
	{
	"type": "page",
	"title": "{{ toponymy.title }}",
	"categories": [{% for cat in toponymy.categories %}"{{ cat }}"{% unless forloop.last %},{% endunless %}{% endfor %}],	
	"tags": [{% for tag in toponymy.tags %}"{{ tag }}"{% unless forloop.last %},{% endunless %}{% endfor %}],
	"url": "{{ toponymy.url }}",
	"content":{{ toponymy.content | strip_html | json_escape | jsonify}},
	"excerpt": {{ toponymy.excerpt | rstrip | json_escape | jsonify }},
	"description": {{ toponymy.description | json_escape | jsonify }}
	}
	{% endfor %}
]
